[user]
	email = prajjwalt9@gmail.com
	name = MISSIC17
[alias]
	adog = log --all --decorate --oneline --graph
	adog10 = log -n 10 --decorate --oneline --graph
	graph = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(auto)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)'

[init]
	defaultBranch = main
[core]
	editor = vim
[credential "https://github.com"]
	helper = 
	helper = !/usr/bin/gh auth git-credential
[credential "https://gist.github.com"]
	helper = 
	helper = !/usr/bin/gh auth git-credential
[hub]
	protocol = https
[rerere]
	enabled = true
[column]
	ui = auto
[branch]
	sort = -committerdate
